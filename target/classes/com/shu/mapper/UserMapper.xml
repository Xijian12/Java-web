<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.shu.mapper.UserMapper">
    <select id="findByEmail" resultType="com.shu.pojo.User">
        select * from user where email=#{email}
    </select>

<!--    <insert id="add" parameterType="com.shu.pojo.User" useGeneratedKeys="true" keyProperty="id">-->
<!--        Insert into user(account, password, nickname, points, email)-->
<!--        values(#{account}, #{password}, #{nickname}, #{points,jdbcType=INTEGER}, #{email})-->
<!--    </insert>-->
    <insert id="add" parameterType="com.shu.pojo.User">
        Insert into user(user_account, user_password, nickname, points, email)
        values(#{user_account}, #{user_password}, #{nickname}, #{points, jdbcType=INTEGER}, #{email})
    </insert>

</mapper>



